{
  "settings": {
    "crawlDelay": false
  },
  "start": [
    {
      "url": "file://test/static/Article.html",
      "is": "content/article",
      "extract": [
        {
          "selector": "body > main",
          "extract": "components",
          "as": "entity.content"
        }
      ]
    }
  ],
  "components": [
    {
      "selector": "header.jumbotron > .container *:not(h1)",
      "extract": "text_single",
      "as": "component.Lede.text"
    }
  ]
}
